<Window x:Class="rlel.update"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Update Available!" Height="300" Width="400" WindowStartupLocation="CenterScreen">
    <Grid>
        <TextBlock Margin="29,50,38,163">An update is available at <Hyperlink Name="msi" Click="msi_Click">http://rlel.frosty-nee.net/rlel.msi</Hyperlink></TextBlock>
        <TextBlock Margin="29,113,38,33">You can read about any changes made to rlel on <Hyperlink Name="github" Click="github_Click" NavigateUri="http://github.com/frostbite/rlel">Github</Hyperlink></TextBlock>
        <Button Name="ok" Content="OK" Height="23" Width="60" Margin="150,174,168,64" Click="ok_Click"/>
    </Grid>
</Window>
